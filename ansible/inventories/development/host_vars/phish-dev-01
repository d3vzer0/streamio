ansible_host: &mgt_addr 192.168.90.248 # Example of IP of TestVM, replace this with target host IP

# Firewall rule included but not deployed, use for own reference
firewall:
  defaults:
    - direction: incoming
      policy: allow
  rules:
    - rule: allow
      to_port: 22
      protocol: tcp
    - rule: allow
      to_port: 80
      protocol: tcp